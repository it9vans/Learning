<Page x:Class="Main_Project.ResultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Main_Project"
      mc:Ignorable="d" 
      d:DesignHeight="750" d:DesignWidth="1050"
      Title="ResultsPage" Height="750" Width="1050"
      Background="White">

    <Grid>
        <Button x:Name="ButtonBack" Style ="{StaticResource MainButtonTemplate}" Grid.ColumnSpan="2" Content="Назад" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="32,31,0,0" VerticalAlignment="Top" Width="110" Height="55" FontSize="30" RenderTransformOrigin="0.539,0.121" Foreground="White" BorderBrush="{x:Null}" FontFamily="Segoe UI" Click="ClickBackButton"/>
        <DataGrid x:Name="resultsDataGrid" Margin="132,146,130,150" FontFamily="Segoe UI" FontSize="20"/>
    </Grid>
</Page>
