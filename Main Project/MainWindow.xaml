            </Canvas>
        </Viewbox>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Width="1050" Margin="300,0,0,0"/>
        <Button x:Name="buttonAccount" Content="Button" Style="{StaticResource TransparentButtonTemplate}" HorizontalAlignment="Left" Margin="33,608,0,0" VerticalAlignment="Top" Height="76" Width="234" FontSize="30"/>
        <Button x:Name="buttonResults" Content="Результаты" Style="{StaticResource TransparentButtonTemplate}" HorizontalAlignment="Left" Margin="33,502,0,0" VerticalAlignment="Top" Height="76" Width="234" FontSize="30" Click="ClickButtonResults"/>
        <Border Margin="54,23,1112,526" CornerRadius="15" BorderThickness="1">
            <Image Source="Images/Learning Math logo1.png" HorizontalAlignment="Center" VerticalAlignment="Center" Width="184" Height="184" RenderTransformOrigin="1.27,0.665"/>
        </Border>
    </Grid>
</Window>
